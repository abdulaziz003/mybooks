<h2>Search Author</h2>
<form action="/authors" method="get">
  <label for="author-name">Name</label>
  <input type="text" name="name" id="author-name" value="<%= searchOptions.name%>">
  <button type="submit">Search</button>
</form>
<br>
<% authors.forEach(author => { %>
  <div><%= author.name %></div>
<% }) %>