<h2>Search Book</h2>
<form action="/books" method="get">
  <div>
    <label for="title">Title</label>
    <input type="text" name="title" id="title" value="<%= searchOptions.title%>">
  </div>
  <div>
    <label for="published-after">Published After</label>
    <input type="date" name="publishedAfter" id="publish-after" value="<%= searchOptions.publishedAfter%>">
  </div>
  <div>
    <label for="published-before">Published Before</label>
    <input type="date" name="publishedBefore" id="publish-before" value="<%= searchOptions.publishedBefore%>">
  </div>
  <button type="submit">Search</button>
</form>
<div>
<% books.forEach(book => { %>
  <img width="100" height="150" src="<%= book.coverImagePath %>" alt="">
<% }) %>
</div>